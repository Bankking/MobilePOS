<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/inventoryAddMoreProductT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="17dp"
        android:layout_y="20dp"
        android:text="Add More Product"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/inventoryAMPBrandT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="17dp"
        android:layout_y="60dp"
        android:text="Brand :" />

    <TextView
        android:id="@+id/inventoryAMPPriceT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="17dp"
        android:layout_y="210dp"
        android:text="Price :" />

    <TextView
        android:id="@+id/inventoryAMPNameT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="17dp"
        android:layout_y="110dp"
        android:text="Name :" />

    <TextView
        android:id="@+id/inventoryAMPSpecT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="17dp"
        android:layout_y="260dp"
        android:text="Spec :" />

    <TextView
        android:id="@+id/inventoryAMPQuantityT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="17dp"
        android:layout_y="150dp"
        android:text="Quantity :" />

    <EditText
        android:id="@+id/invetoryAMPBrandTF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="70dp"
        android:layout_y="60dp"
        android:ems="10" />
    
     <EditText
        android:id="@+id/invetoryAMPNameTF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="70dp"
        android:layout_y="110dp"
        android:ems="10" />
     
      <EditText
        android:id="@+id/invetoryAMPPriceTF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="70dp"
        android:layout_y="160dp"
        android:ems="10" />
      
       <EditText
        android:id="@+id/invetoryAMPQuantityTF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="70dp"
        android:layout_y="210dp"
        android:ems="10" />
       
        <EditText
            android:id="@+id/invetoryAMPSpecTF"
            android:layout_width="wrap_content"
            android:layout_height="115dp"
            android:layout_x="70dp"
            android:layout_y="260dp"
            android:ems="10" />

</AbsoluteLayout>
